{
    "Segment": {
        "prefix": "SEG",
        "body": [
            "${1:SEG-NAME} SEGMENT $2",
            "\t$0",
            "${1:SEG-NAME} ENDS"
        ],
        "description": "定义一个段"
    },
    "Assume": {
        "prefix": "ASSUME",
        "body": [
            "ASSUME CS:${1:CODE}, DS:${2:DATA}, ${3|SS:STACK,ES:DATA|}"
        ]
    },
    "String": {
        "prefix": "STR",
        "body": [
            "${1:STRING} DB $0"
        ],
        "description": "定义一个字符串"
    },
    "Procedure": {
        "prefix": "PROC",
        "body": [
            "${1:PROC-NAME} PROC $2",
            "\t$0",
            "\tRET",
            "${1:PROC-NAME} ENDP"
        ],
        "description": "定义过程"
    },
    "Macro": {
        "prefix": "MACRO",
        "body": [
            "${1:MACRO-NAME} MACRO $2",
            "\t$0",
            "ENDM"
        ],
        "description": "定义宏"
    },
    "Test and Jump": {
        "prefix": "TESTJ",
        "body": [
            "TEST $1, $2",
            "${3:JMP} $4"
        ],
        "description": "测试后转跳"
    },
    "Compare and Jump": {
        "prefix": "CMPJ",
        "body": [
            "${1:CMP} $2, $3",
            "${4:JMP} $5"
        ],
        "description": "比较后转跳"
    },
    "Loop": {
        "prefix": "LOOP",
        "body": [
            "${1:LABEL}: ",
            "\t$0",
            "LOOP ${1:LABEL}"
        ],
        "description": "循环语句"
    },
    "ASM Start":{
        "prefix":"starter",
        "body":[
            "${1:DATA} SEGMENT",
            "\t$2",
            "${1:DATA} ENDS",
            "",
            "${3:STACKS} SEGMENT",
            "\t$4",
            "${3:STACKS} ENDS",
            "",
            "${5:CODES} SEGMENT",
            "\tASSUME CS:${5:CODES}, DS:${1:DATA}, SS:${3:STACKS}",
            "${6:START}:",
            "\tMOV AX, ${1:DATA}",
            "\tMOV DS, AX",
            "\t$0",
            "\tMOV AH, 4CH",
            "\tINT 21H",
            "${5:CODES} ENDS",
            "END ${6:START}"
        ],
        "description":"汇编程序基本框架"
    }
}
