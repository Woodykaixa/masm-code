{
    "Segment": {
        "prefix": "SEG",
        "body": [
            "$1 SEGMENT $2",
            "\t$0",
            "$1 ENDS"
        ],
        "description": "定义一个段"
    },
    "Assume": {
        "prefix": "ASSUME",
        "body": [
            "ASSUME: CS:${1:CODE}, DS:${2:DATA}, SS:${3:STACK}"
        ]
    },
    "String": {
        "prefix": "STR",
        "body": [
            "${1:STRING} DB \"$2\"$0"
        ],
        "description": "定义一个字符串"
    },
    "Move": {
        "prefix": "MOV",
        "body": [
            "MOV $1, $2"
        ]
    },
    "Procedure": {
        "prefix": "PROC",
        "body": [
            "$1 PROC $2",
            "\t$0",
            "\tRET",
            "$1 ENDP"
        ],
        "description": "定义过程"
    },
    "Macro": {
        "prefix": "MACRO",
        "body": [
            "$1 MACRO $2",
            "\t$0",
            "$1 ENDM"
        ],
        "description": "定义宏"
    },
    "Test and Jump": {
        "prefix": "TESTJ",
        "body": [
            "TEST $1, $2",
            "${3:JMP} $4"
        ],
        "description": "测试后转跳"
    },
    "Compare and Jump": {
        "prefix": "CMPJ",
        "body": [
            "${1:CMP} $2, $3",
            "${4:JMP} $5"
        ],
        "description": "比较后转跳"
    },
    "Loop": {
        "prefix": "LOOP",
        "body": [
            "$1: ",
            "\t$0",
            "${2:LOOP} $1"
        ],
        "description": "循环语句"
    }
}